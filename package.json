{
  "name": "@ristostevcev/bs-declaredom",
  "version": "1.0.0",
  "scripts": {
    "clean": "bsb -clean-world",
    "build": "bsb -make-world",
    "watch": "bsb -make-world -w",
    "test:node": "node lib/js/test/nodes/Test_Nodes.bs.js | tap-spec",
    "test:css": "node lib/js/test/TestCss.bs.js | tap-spec",
    "test:css:media": "node lib/js/test/Test_Css_Media.bs.js | tap-spec",
    "test:css:property": "node lib/js/test/Test_Css_Property.bs.js | tap-spec",
    "test:css:stylesheet": "node lib/js/test/Test_Css_Stylesheet.bs.js | tap-spec",
    "test": "npm run test:node && npm run test:css && npm run test:css:media && npm run test:css:property && npm run test:css:stylesheet",
    "graph:dot": "bs-dependency-graph | json-to-dot > graph.dot",
    "graph:image": "dot -Tpng graph.dot -o graph.png",
    "graph": "npm run graph:dot && npm run graph:image",
    "docs": "bs-odoc",
    "example": "parcel example/example.html"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "Risto Stevcev",
  "license": "MIT",
  "devDependencies": {
    "@ristostevcev/bs-callbag-basics": "^1.2.0",
    "@ristostevcev/bs-odoc": "^1.2.0",
    "@ristostevcev/bs-tape": "^0.1.0",
    "bs-dependency-graph": "^1.2.0",
    "bs-platform": "^4.0.3",
    "jsdom": "^13.0.0",
    "json-to-dot": "^1.1.0",
    "parcel-bundler": "^1.10.3",
    "tap-spec": "^5.0.0"
  },
  "dependencies": {
    "@ristostevcev/bs-callbag": "^1.1.0",
    "@webcomponents/webcomponentsjs": "^2.1.3",
    "bs-webapi": "^0.13.0",
    "callbag-element": "^1.0.0",
    "callbag-html": "^0.1.0",
    "declaredom": "^1.1.0"
  }
}
